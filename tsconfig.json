{
  // NOTE: These options are overridden in webpack.config.js.
  "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "moduleResolution": "node",
      "sourceMap": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noEmitOnError": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "removeComments": false,
      "jsx": "react",
      "outDir": "dist",
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "skipLibCheck": true,
      "isolatedModules": true
  },
  "exclude": [
      "node_modules"
  ]
}
